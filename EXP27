#include <stdio.h>

int main() {
    int a[50], n, pos, val, i, ch;
    scanf("%d", &n);
    for(i=0;i<n;i++) scanf("%d",&a[i]);
    scanf("%d%d",&pos,&val);
    for(i=n;i>=pos;i--) a[i]=a[i-1];
    a[pos]=val; n++;
    scanf("%d",&pos);
    for(i=pos;i<n;i++) a[i]=a[i+1]; 
    n--;
    for(i=0;i<n;i++) printf("%d ",a[i]);
}
